<?xml version="1.0"?>
<wsdl:definitions name="Employee Payroll"
                  targetNamespace="http://payroll.com/services/employee"
                  xmlns:tns="http://payroll.com/services/employee"
                  xmlns:types="http://payroll.com/employee"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
                  
	<wsdl:types>
        <xs:schema
                targetNamespace="http://payroll.com/employee"
                xmlns:xs="http://www.w3.org/2001/XMLSchema"
                xmlns:tns="http://payroll.com/employee"
                elementFormDefault="qualified"
                version="1.0">
           	
           	<xs:complexType name="employeeInfo">
		        <xs:sequence>
		           <xs:element name="id" type="xs:long"/>
		                <xs:element name="name" type="xs:string"/>
		                <xs:element name="department" type="xs:string"/>
		                <xs:element name="salary" type="xs:double"/>
		        </xs:sequence>
		    </xs:complexType>
		    
		    <xs:complexType name="employeeInfoWithoutId">
		        <xs:sequence>
		                <xs:element name="name" type="xs:string"/>
		                <xs:element name="department" type="xs:string"/>
		                <xs:element name="salary" type="xs:double"/>
		        </xs:sequence>
		    </xs:complexType>
    
	        <xs:element name="addEmployeeRequest">
		        <xs:complexType>
		            <xs:sequence>
		                <xs:element name="name" type="xs:string"/>
		                <xs:element name="department" type="xs:string"/>
		                <xs:element name="salary" type="xs:double"/>
		            </xs:sequence>
		        </xs:complexType>
	    	</xs:element>
	    	
	    	<xs:complexType name="serviceStatus">
		        <xs:sequence>
		            <xs:element name="statusCode" type="xs:string"/>
		            <xs:element name="message" type="xs:string"/>
		        </xs:sequence>
    		</xs:complexType> 
    		
    		<xs:element name="addEmployeeResponse">
		        <xs:complexType>
		            <xs:sequence>
		                <xs:element name="serviceStatus" type="tns:serviceStatus"/>            
		                <xs:element name="employeeInfo" type="tns:employeeInfo"/>
		            </xs:sequence>
		        </xs:complexType>
		    </xs:element>
		    
		    <xs:element name="deleteEmployeeRequest">
		        <xs:complexType>
		            <xs:sequence>
		               <xs:element name="id" type="xs:long"/>
		            </xs:sequence>
		        </xs:complexType>
		    </xs:element>
		    
		    <xs:element name="deleteEmployeeResponse">
		        <xs:complexType>
		            <xs:sequence>
		                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
		            </xs:sequence>
		        </xs:complexType>
		    </xs:element>   
		    
		    <xs:element name="getEmployeeByIdRequest">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="id" type="xs:long" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="getEmployeeByIdResponse">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="employeeInfo" type="tns:employeeInfo" />
					</xs:sequence>
				</xs:complexType>
			</xs:element> 
			
			 <xs:element name="getAllEmployeeRequest">
		        <xs:complexType/>
		    </xs:element>    
		    <xs:element name="getAllEmployeeResponse">
		        <xs:complexType>
		            <xs:sequence>
		                <xs:element name="employeeInfo" type="tns:employeeInfo" maxOccurs="unbounded"/>
		            </xs:sequence>
		        </xs:complexType>
		    </xs:element> 
		    
		    <xs:element name="updateEmployeeRequest">
		        <xs:complexType>
		            <xs:sequence>
		               	<xs:element name="employeeInfo" type="tns:employeeInfo" />
		            </xs:sequence>
		        </xs:complexType>
		    </xs:element>
		    <xs:element name="updateEmployeeResponse">
		        <xs:complexType>
		            <xs:sequence>
		                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
		            </xs:sequence>
		        </xs:complexType>
		    </xs:element>      
		    
		</xs:schema>
    </wsdl:types>
    
    <wsdl:message name="addEmployeeRequest">
        <wsdl:part name="addEmployeeRequest" element="types:addEmployeeRequest" />
    </wsdl:message>

    <wsdl:message name="addEmployeeResponse">
        <wsdl:part name="addEmployeeResponse" element="types:addEmployeeResponse" />
    </wsdl:message>
    
    <wsdl:message name="deleteEmployeeRequest">
        <wsdl:part name="deleteEmployeeRequest" element="types:deleteEmployeeRequest" />
    </wsdl:message>
    
    <wsdl:message name="deleteEmployeeResponse">
        <wsdl:part name="deleteEmployeeResponse" element="types:deleteEmployeeResponse" />
    </wsdl:message>
    
    <wsdl:message name="getEmployeeByIdRequest">
        <wsdl:part name="getEmployeeByIdRequest" element="types:getEmployeeByIdRequest" />
    </wsdl:message>
    
    <wsdl:message name="getEmployeeByIdResponse">
        <wsdl:part name="getEmployeeByIdResponse" element="types:getEmployeeByIdResponse" />
    </wsdl:message>
    
    <wsdl:message name="getAllEmployeeRequest">
        <wsdl:part name="getAllEmployeeRequest" element="types:getAllEmployeeRequest" />
    </wsdl:message>
    
    <wsdl:message name="getAllEmployeeResponse">
        <wsdl:part name="getAllEmployeeResponse" element="types:getAllEmployeeResponse" />
    </wsdl:message>
    
    <wsdl:message name="updateEmployeeRequest">
        <wsdl:part name="updateEmployeeRequest" element="types:updateEmployeeRequest" />
    </wsdl:message>
    
    <wsdl:message name="updateEmployeeResponse">
        <wsdl:part name="updateEmployeeResponse" element="types:updateEmployeeResponse" />
    </wsdl:message>
    
    <wsdl:portType name="EmployeePort">
		<wsdl:operation name="addEmployee">
			<wsdl:input message="tns:addEmployeeRequest" name="addEmployeeRequest"> </wsdl:input>
			<wsdl:output message="tns:addEmployeeResponse" name="addEmployeeResponse"> </wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="deleteEmployee">
			<wsdl:input message="tns:deleteEmployeeRequest" name="deleteEmployeeRequest"> </wsdl:input>
			<wsdl:output message="tns:deleteEmployeeResponse" name="deleteEmployeeResponse"> </wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="getEmployeeById">
			<wsdl:input message="tns:getEmployeeByIdRequest" name="getEmployeeByIdRequest"> </wsdl:input>
			<wsdl:output message="tns:getEmployeeByIdResponse" name="getEmployeeByIdResponse"> </wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="getAllEmployee">
			<wsdl:input message="tns:getAllEmployeeRequest" name="getAllEmployeeRequest"> </wsdl:input>
			<wsdl:output message="tns:getAllEmployeeResponse" name="getAllEmployeeResponse"> </wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="updateEmployee">
			<wsdl:input message="tns:updateEmployeeRequest" name="updateEmployeeRequest"> </wsdl:input>
			<wsdl:output message="tns:updateEmployeeResponse" name="updateEmployeeResponse"> </wsdl:output>
		</wsdl:operation>
	</wsdl:portType>
	
	<wsdl:binding name="EmployeeBinding" type="tns:EmployeePort">
	<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="addEmployee">
		<soap:operation soapAction=""/>
			<wsdl:input name="addEmployeeRequest">
			<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output name="addEmployeeResponse">
			<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
		
		<wsdl:operation name="deleteEmployee">
		<soap:operation soapAction=""/>
			<wsdl:input name="deleteEmployeeRequest">
			<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output name="deleteEmployeeResponse">
			<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
		
		<wsdl:operation name="getEmployeeById">
		<soap:operation soapAction=""/>
			<wsdl:input name="getEmployeeByIdRequest">
			<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output name="getEmployeeByIdResponse">
			<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
		
		<wsdl:operation name="getAllEmployee">
		<soap:operation soapAction=""/>
			<wsdl:input name="getAllEmployeeRequest">
			<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output name="getAllEmployeeResponse">
			<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
		
		<wsdl:operation name="updateEmployee">
		<soap:operation soapAction=""/>
			<wsdl:input name="updateEmployeeRequest">
			<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output name="updateEmployeeResponse">
			<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
		
	</wsdl:binding>
	
	<wsdl:service name="EmployeePayrollPortService">
		<wsdl:port binding="tns:EmployeeBinding" name="EmployeeBinding">
		<soap:address location="http://localhost:8080/ws/employeePayroll"/>
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>